{
  "version": 2,
  "functions": {
    "api/test.py": {
      "runtime": "@vercel/python@3.10.5"
    }
  },
  "outputDirectory": "frontend/dist",
  "buildCommand": "cd frontend && npm install && npm run build",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}